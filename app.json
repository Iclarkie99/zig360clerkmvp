{
  "expo": {
    "name": "ZIG360clerkmvp",
    "slug": "ZIG360clerkmvp",
    "version": "1.0.0",
    "scheme": "com.zig360.zig360clerkmvp",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/zig360-static-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F7F7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
      "infoPlist": {
        "NSContactsUsageDescription": "Allow $(PRODUCT_NAME) to access your contacts to add them to your plans.",
        "CFBundleIdentifier": "com.zig360.zig360clerkmvp"
      },
      "bundleIdentifier": "com.zig360.zig360clerkmvp"
    },
    "android": {
      "package": "com.zig360.zig360clerkmvp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#F7F7F7"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": "2.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts to add them to your plans."
        }
      ],
      [
        "expo-updates",
        {
          "username": "iain-zig360"
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "0c60a489-060b-41bb-b573-a7261b91e068"
      }
    },
    "owner": "iain-zig360"
  }
}
